{
  "name": "ypbooking-admin2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start --env PORT=3000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint --cache src/**/*.{ts,tsx,js,jsx}",
    "css": "stylelint --cache src/**/*.{ts,tsx,js,jsx,html,css,scss}",
    "lint:fix": "eslint --cache --fix src/**/*.{ts,tsx,js,jsx}",
    "types": "tsc --pretty --noEmit",
    "check-format": "prettier --cache --check src/**/*.{ts,tsx,js,jsx,json,css,scss,md}",
    "format": "prettier --check --write --cache src/**/*.{ts,tsx,js,jsx,json,css,scss,md}",
    "check-all": "yarn check-format && yarn check-types && yarn lint && yarn build",
    "dev": "json-server -p 3500 -i _id -w db.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-svg": "^5.0.0",
    "axios": "^1.4.0",
    "date-fns": "^2.29.1",
    "modern-normalize": "^1.1.0",
    "polished": "^4.2.2",
    "react": "^18.1.0",
    "react-content-loader": "^6.2.0",
    "react-datepicker": "^4.14.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.2",
    "react-is": "^18.2.0",
    "react-loader-spinner": "^5.2.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-scroll-to-bottom": "^4.2.0",
    "react-svg": "^16.1.17",
    "react-toastify": "^9.0.4",
    "react-transition-group": "^4.4.2",
    "react-window": "^1.8.7",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.52.2",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
